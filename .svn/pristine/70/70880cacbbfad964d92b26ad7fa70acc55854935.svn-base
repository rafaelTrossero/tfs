            <div class="testimonials">
                <div class="container"> 

                    <h3><span>Proyectos de</span>Investigacion</h3>
                    <p class="dolore">A continuacion se muestra la lista de proyectos de investigacion en los que tienen participacion los docentes del departamento de informatica
                    </p>
                    <br></br>





                    <!-- /.row -->
                

                <!-- /.row -->
                <h:form id="form">




                    <h:dataTable id="dtProyectosInvestigacion"
                                 styleClass="table"

                                 rows="50"

                                 value="#{listaProyectosInvestigacionBean.lstProyectoInvestigacion}"
                                 var="item"    >



                        <p:column  width="300"   filterBy="#{item.titulo}" sortBy="#{item.titulo}">
                            <f:facet name="header">
                                <h:outputText value="Titulo del Proyecto"/>
                            </f:facet>
                            <h:outputText value="#{item.titulo}"/>
                        </p:column> 

                        <p:column  width="300"   filterBy="#{item.fecha_presentacion}" sortBy="#{item.fecha_presentacion}">
                            <f:facet name="header">
                                <h:outputText value="Fecha dePresentacion del Proyecto "/>
                            </f:facet>
                            <h:outputText value="#{item.fecha_presentacion}">
                                <f:convertDateTime pattern="dd/MM/yyyy" />
                            </h:outputText>
                        </p:column> 

                        <p:column  width="300"  filterBy="#{item.carrera.carrera}" sortBy="#{item.carrera.carrera}">
                            <f:facet name="header">
                                <h:outputText value="Carrera"/>
                            </f:facet>
                            <h:outputText value="#{item.carrera.carrera}"/>
                        </p:column> 

                        <p:column  width="300"  filterBy="#{item.director.apellido}" sortBy="#{item.director.apellido}">
                            <f:facet name="header">
                                <h:outputText value="Director del Proyecto"/>
                            </f:facet>
                            <h:outputText value="#{item.director.nombre},#{item.director.apellido} "/>
                        </p:column> 




                        <p:column  width="300">
                            <f:facet name="header">
                                <h:outputText value="E-mail"/>
                            </f:facet>
                            <h:outputText value="#{item.director.email}"/>
                        </p:column> 

                        <p:column  width="200">
                            <f:facet name="header">
                                <h:outputText value="Descarga de archivo pdf"/>
                            </f:facet>
                            <h:commandButton value="Download" action="#{fileDownloadView.downloadFile(listaProyectosInvestigacionBean.selectedProyectoInvestigacion.titulo)}"/>

                        </p:column> 

                    </h:dataTable>  



                </h:form>

