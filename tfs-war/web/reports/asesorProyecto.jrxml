<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="report name" pageWidth="595" pageHeight="842" whenNoDataType="BlankPage" columnWidth="535" leftMargin="20" rightMargin="20" topMargin="2" bottomMargin="2">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="idProyecto" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
     proyecto.ID AS proyecto_ID,
     persona.ID AS persona_ID,
     persona.APELLIDO AS persona_APELLIDO,
     persona.NOMBRE AS persona_NOMBRE,
     proyectoasesor.ID AS proyectoasesor_ID,
     proyectoasesor.profesional_id AS proyectoasesor_profesional_id,
     proyectoasesor.proyecto_id AS proyectoasesor_proyecto_id
FROM
      proyecto INNER JOIN  proyectoasesor ON proyecto.ID = proyectoasesor.proyecto_id
     INNER JOIN persona ON proyectoasesor.profesional_id = persona.ID
WHERE
     proyecto.ID = $P{idProyecto} AND proyectoasesor.ACTIVE=TRUE]]>
	</queryString>
	<field name="proyecto_id" class="java.lang.Integer"/>
	<field name="persona_id" class="java.lang.Long"/>
	<field name="persona_apellido" class="java.lang.String"/>
	<field name="persona_nombre" class="java.lang.String"/>
	<field name="proyectoasesor_id" class="java.lang.Integer"/>
	<field name="proyectoasesor_profesional_id" class="java.lang.Long"/>
	<field name="proyectoasesor_proyecto_id" class="java.lang.Long"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<columnHeader>
		<band height="21" splitType="Stretch">
			<staticText>
				<reportElement x="0" y="1" width="100" height="20"/>
				<textElement/>
				<text><![CDATA[Asesor:]]></text>
			</staticText>
		</band>
	</columnHeader>
	<detail>
		<band height="20" splitType="Stretch">
			<textField>
				<reportElement x="0" y="0" width="520" height="20"/>
				<textElement/>
				<textFieldExpression><![CDATA[$F{persona_nombre}+", "+$F{persona_apellido}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
